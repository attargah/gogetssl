{
    "name": "attargah/gogetssl",
    "description": "GogetSSL API integration for Laravel framework",
    "keywords": [
        "attargah",
        "gogetssl"
    ],
    "version": "v1.0.1",
    "homepage": "https://github.com/attargah/gogetssl",
    "license": "MIT",
    
    "authors": [
        {
            "name": "Oguz Kucuk",
            "email": "koguz168@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Attargah\\GogetSSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Attargah\\GogetSSL\\Tests\\": "tests",
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attargah\\GogetSSL\\GogetSSLServiceProvider"
            ],
            "aliases": {
                "GogetSSL": "Attargah\\GogetSSL\\Facades\\GogetSSL"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
